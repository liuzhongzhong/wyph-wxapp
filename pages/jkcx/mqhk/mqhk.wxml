<!--pages/yqcx/value/value.wxml-->
<view class="container">

  <view class='title'>
    <text>其它信息</text>
  </view>

  <view class="value">
    <view class='line'>
      <view class='key'>
        <text>每期本金</text>
      </view>
      <view class='item'>
        <text>{{every_principal}}元</text>
      </view>
    </view>
    <view class='line' bindtap='bindPhone'>
      <view class='key'>
        <text>每期利息</text>
      </view>
      <view class='item'>
        <text>{{cyc_interest}}元</text>
      </view>
    </view>
  </view>

  
</view>
